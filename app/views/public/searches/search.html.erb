<% if @range == "User" %>
  <% @users.each do |user| %>
    <%= render "public/users/index", users: @users %>
  <% end %>
<% elsif @range == "Article" %>
  <% @articles.each do |article| %>
    <%= render "public/articles/index", articles: @articles %>
  <% end %>
<% else @range == "Tag" %>
  <% @tags.each do |tag| %>
    <%= tag.name %>
    タグ
  <% end %>
<% end %>
