<% if @range == "User" %>
  <% @users.each do |user| %>
    <%= user.screen_name %>
    ユーザー
  <% end %>
<% elsif @range == "Article" %>
  <% @articles.each do |article| %>
    <%= article.title %>
    記事
  <% end %>
<% else @range == "Tag" %>
  <% @tags.each do |tag| %>
    <%= tag.name %>
    タグ
  <% end %>
<% end %>
